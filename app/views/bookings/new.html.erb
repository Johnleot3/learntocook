
<div class="container-bookings" style="background-color:
   rgba(255, 255, 255, 0.2) 50%)">

    <div class="row">
      <div class="col-xs-12">
<div class="col-xs-12 col-sm-4 col-sm-offset-4">
  <h1 class="heading-show">Create a New Booking</h1>
<p class="text">Check your dashboard to see all your bookings</p>
<div class="box-book">
  <%= simple_form_for [@kitchen, @booking] do |f| %>
  <%= f.input :date %>
</div>
<div class="row-book">
      <%= f.submit "Book", class: "btn-small" %>
       </div>
<% end %>
</div>
</div>
</div>
</div>


